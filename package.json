{
  "name": "animal-manager-web",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14.13"
  },
  "scripts": {
    "prestart": "yarn generate",
    "prebuild": "yarn generate",
    "preinstall": "node set-node-version.js",
    "start": "env-cmd -f environments/.env.${APP_NAME:='alpaca'}.local react-app-rewired start",
    "build": "env-cmd -f environments/.env.${APP_NAME:='alpaca'}.${BUILD_ENV:='dev'} react-app-rewired build",
    "start:alpaca": "APP_NAME=alpaca yarn start",
    "start:llama": "APP_NAME=llama yarn start",
    "start:sheep": "APP_NAME=sheep yarn start",
    "build:alpaca:dev": "APP_NAME=alpaca BUILD_ENV=dev yarn build",
    "build:llama:dev": "APP_NAME=llama BUILD_ENV=dev yarn build",
    "build:sheep:dev": "APP_NAME=sheep BUILD_ENV=dev yarn build",
    "build:alpaca:release": "APP_NAME=alpaca BUILD_ENV=production yarn build",
    "build:llama:release": "APP_NAME=llama BUILD_ENV=production yarn build",
    "build:sheep:release": "APP_NAME=sheep BUILD_ENV=production yarn build",
    "clean": "rm -rf node_modules yarn.lock package-lock.json src/generated; yarn;",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "lint": "eslint --cache --fix --max-warnings 0 --no-ignore",
    "format": "prettier --write \"**/*.+(json)\"",
    "generate": "env-cmd --file=environments/.env.${APP_NAME:='alpaca'}.dev graphql-codegen --config codegen.yml"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.15",
    "@sentry/react": "^6.8.0",
    "@sentry/tracing": "^6.8.0",
    "@stripe/stripe-js": "^1.15.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "babel-jest": "26.6.0",
    "bootstrap": "^4.6.0",
    "classnames": "^2.3.1",
    "date-fns": "^2.22.1",
    "draft-js": "^0.11.7",
    "formik": "^2.2.6",
    "ga-4-react": "^0.1.281",
    "graphql": "^15.5.0",
    "i18next": "^20.2.2",
    "i18next-browser-languagedetector": "^6.1.0",
    "lodash": "^4.17.21",
    "rc-pagination": "^3.1.9",
    "react": "^17.0.2",
    "react-apple-login": "^1.1.3",
    "react-avatar": "^3.10.0",
    "react-content-loader": "^6.0.3",
    "react-dom": "^17.0.2",
    "react-draft-wysiwyg": "^1.14.7",
    "react-dropzone": "^11.3.4",
    "react-easy-crop": "^3.5.2",
    "react-helmet": "^6.1.0",
    "react-hot-toast": "^2.1.1",
    "react-i18next": "^11.8.15",
    "react-lazy-load-image-component": "^1.5.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-select": "^4.3.1",
    "react-switch": "^6.0.0",
    "react-table": "^7.7.0",
    "react-use": "^17.2.4",
    "reactstrap": "^8.9.0",
    "reactstrap-date-picker": "^0.0.11",
    "stripe": "^8.157.0",
    "subscriptions-transport-ws": "^0.9.19",
    "typescript": "^4.1.2",
    "waitlistapi": "^0.6.2",
    "web-vitals": "^1.0.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.4",
    "@graphql-codegen/fragment-matcher": "^2.0.1",
    "@graphql-codegen/typescript": "1.22.0",
    "@graphql-codegen/typescript-operations": "1.17.16",
    "@graphql-codegen/typescript-react-apollo": "2.2.4",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.169",
    "@types/node": "^12.0.0",
    "@types/node-sass": "^4.11.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-draft-wysiwyg": "^1.13.2",
    "@types/react-helmet": "^6.1.1",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-select": "^4.0.15",
    "@types/react-table": "^7.7.2",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "copy-webpack-plugin": "^6.3.2",
    "env-cmd": "^10.1.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "lint-staged": ">=10",
    "node-filter-async": "^2.0.0",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "prettier-stylelint": "^0.4.2",
    "react-app-rewired": "^2.1.8",
    "stylelint": "^13.13.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-declaration-use-variable": "^1.7.2",
    "stylelint-high-performance-animation": "^1.5.2",
    "stylelint-prettier": "^1.2.0"
  }
}
